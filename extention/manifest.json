{
    "manifest_version": 3,
    "name": "Chatbot Extension with Authentication",
    "version": "1.0",
    "description": "A chatbot extension with login and registration screens",
    "permissions": ["storage", "tabs"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.jpg",
        "48": "icon.jpg",
        "128": "icon.jpg"
      }
    }
  }
  